{
  "name": "marty-test",
  "version": "0.0.1",
  "homepage": "",
  "repository": {
    "type": "git",
    "directory": ""
  },
  "engines": {
    "node": ">= 16.3.0"
  },
  "description": "",
  "author": "",
  "license": "MIT",
  "private": false,
  "main": "./index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./index.js",
  "dependencies": {
    "handlebars": "^4.4.3"
  },
  "scripts": {
    "start": "run-p start:*",
    "start:build-watch": "run-p build-watch:*",
    "start:example": "cd example && npm run start",
    "build": "run-s build:*",
    "build:babel": "babel src --out-dir ./dist --extensions '.ts'",
    "build-watch:babel": "babel src --watch --out-dir ./dist --extensions '.ts'",
    "build:ts": "tsc",
    "build-watch:ts": "tsc -w",
    "build:webpack": "webpack",
    "build-watch:webpack": "webpack --watch --config webpack.dev.config.js",
    "test": "jest --config jest.config.js",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"src/**/*.js\"",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint src --ext '.ts'",
    "lint:prettier": "prettier src/*.ts",
    "lint-staged": "lint-staged",
    "typecheck": "tsc --noEmit true --emitDeclarationOnly false",
    "prepublishOnly": "run-s lint typecheck format build"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "dist/*",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-modules-umd": "^7.14.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "concurrently": "^6.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^3.1.0",
    "jest": "^27.0.4",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-jest": "^27.0.3",
    "typescript": "^3.7.4",
    "typescript-transform-paths": "^3.0.1",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "resolutions": {
    "handlebars": "^4.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint-staged typecheck"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
